<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>adminslide</servlet-name>
        <servlet-class>Admin.adminslide</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_register</servlet-name>
        <servlet-class>user_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_login1</servlet-name>
        <servlet-class>User_login1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminadd</servlet-name>
        <servlet-class>Admin.adminadd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>Admin.adminlogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminslide</servlet-name>
        <url-pattern>/adminslide</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_register</servlet-name>
        <url-pattern>/user_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_login1</servlet-name>
        <url-pattern>/User_login1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminadd</servlet-name>
        <url-pattern>/adminadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
